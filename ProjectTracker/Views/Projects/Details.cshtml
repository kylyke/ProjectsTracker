@model ProjectTracker.Database.Concrete.Project

@{
    ViewData["Title"] = "Details";
}

<h1>@Html.DisplayFor(model => model.ProjectName)</h1>

<div>
    <h4>Detaylar</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <a scope="col">Firma Adı</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.ProjectName)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Başlangıç Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.StartDate)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Güncelleme Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UpdateDate)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Bitiş Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.FinishDate)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Firma İlgili</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CompanyAbout)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">İlgili</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.User.Name)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Telefon Numarası</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.PhoneNumber)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Bakım Anlaşması</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaintenanceAgreement)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Bakım Anlaşması Başlangıç Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaintenanceAgreementStartDate)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Bakım Anlaşması Bitiş Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.MaintenanceAgreementFinishDate)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Güncelleme Anlaşması</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UpdateAgreement)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Güncelleme Anlaşması Başlangıç Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UpdateAgreementStartDate)
        </dd>
        <dt class="col-sm-2">
            <a scope="col">Güncelleme Anlaşması Bitiş Tarihi</a>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.UpdateAgreementFinisDate)
        </dd>

    </dl>
</div>
<div>
    <a class="btn btn-outline-primary" asp-action="Edit" asp-route-id="@Model.Id">Düzenle</a> |
    <a class="btn btn-outline-secondary" asp-action="Index">Geri Dön</a>
</div>
